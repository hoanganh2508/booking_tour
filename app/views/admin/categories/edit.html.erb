<h1>Update category</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for([:admin, @category]) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.submit "Update category", class: "btn btn-primary" %>
      <% end %>
  </div>
</div>
